{
    "manifest_version": 2,
    "name": "BetterBookmarks",
    "permissions": [ 
        "bookmarks",
        "activeTab"
    ],
    "version": "0.1",
    
    "content_scripts": [
  {
    "matches": [
      "<all_urls>"
    ],
    "js": ["getTitleDesc.js"]
  }
],

    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "Sort bookmarks",
        "default_popup": "popup.html"
    }
    
}
